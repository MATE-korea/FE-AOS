<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/loginFragment">

    <fragment
        android:id="@+id/loginFragment"
        android:name="com.mate.carpool.ui.fragment.us1.LoginFragment"
        android:label="LoginFragment"
        tools:layout="@layout/fragment_login" >
        <action
            android:id="@+id/action_loginFragment_to_RegisterInfoFragment"
            app:destination="@id/RegisterInfoFragment" />
        <action
            android:id="@+id/action_loginFragment_to_loginStartFragment"
            app:destination="@id/loginStartFragment" />
        <action
            android:id="@+id/action_loginFragment_to_RegisterTypeFragment"
            app:destination="@id/RegisterTypeFragment" />
    </fragment>

    <fragment
        android:id="@+id/RegisterInfoFragment"
        android:name="com.mate.carpool.ui.fragment.us2.RegisterInfoFragment"
        android:label="RegisterFragment3"
        tools:layout="@layout/fragment_register_info">

        <action
            android:id="@+id/action_RegisterInfoFragment_to_RegisterPhoneFragment"
            app:destination="@id/RegisterPhoneFragment" />
    </fragment>
    <fragment
        android:id="@+id/RegisterPhoneFragment"
        android:name="com.mate.carpool.ui.fragment.us2.RegisterPhoneFragment"
        android:label="RegisterFragment4"
        tools:layout="@layout/fragment_register_phone">

        <action
            android:id="@+id/action_RegisterPhoneFragment_to_RegisterTypeFragment"
            app:destination="@id/RegisterTypeFragment" />
    </fragment>
    <fragment
        android:id="@+id/RegisterTypeFragment"
        android:name="com.mate.carpool.ui.fragment.us2.RegisterTypeFragment"
        android:label="LoginTypeFragment"
        tools:layout="@layout/fragment_register_type">

        <action
            android:id="@+id/action_RegisterTypeFragment_to_RegisterProfileFragment"
            app:destination="@id/RegisterProfileFragment" />
    </fragment>
    <fragment
        android:id="@+id/RegisterProfileFragment"
        android:name="com.mate.carpool.ui.fragment.us2.RegisterProfileFragment"
        android:label="RegisterProfileFragment"
        tools:layout="@layout/fragment_register_profile">

        <action
            android:id="@+id/action_RegisterProfileFragment_to_RegisterSelectDayFragment"
            app:destination="@id/RegisterSelectDayFragment" />
    </fragment>
    <fragment
        android:id="@+id/RegisterSelectDayFragment"
        android:name="com.mate.carpool.ui.fragment.us2.RegisterSelectDayFragment"
        android:label="LoginSelectDayFragment"
        tools:layout="@layout/fragment_register_select_day" >
        <action
            android:id="@+id/action_RegisterSelectDayFragment_to_loginFragment"
            app:destination="@id/loginFragment"
            app:popUpTo="@id/loginFragment"
            app:popUpToInclusive="true"/>
    </fragment>

    <fragment
        android:id="@+id/loginStartFragment"
        android:name="com.mate.carpool.ui.fragment.us1.LoginStartFragment"
        android:label="LoginStartFragment"
        tools:layout="@layout/fragment_login_start" >
        <action
            android:id="@+id/action_loginStartFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
    </fragment>
    <fragment
        android:id="@+id/createTicketBoardingAreaFragment"
        android:name="com.mate.carpool.ui.fragment.us5.CreateTicketBoardingAreaFragment"
        android:label="createTicketBoardingAreaFragment"
        tools:layout="@layout/fragment_create_carpool_ticket_boarding_area">
        <action
            android:id="@+id/action_createTicketBoardingAreaFragment_to_createTicketBoardingTimeFragment"
            app:destination="@id/createTicketBoardingTimeFragment" />
    </fragment>
    <fragment
        android:id="@+id/createTicketBoardingTimeFragment"
        android:name="com.mate.carpool.ui.fragment.us5.CreateTicketBoardingTimeFragment"
        android:label="fragment_create_carpool_ticket_boarding_time"
        tools:layout="@layout/fragment_create_carpool_ticket_boarding_time" >
        <action
            android:id="@+id/action_createTicketBoardingTimeFragment_to_createTicketOpenChatFragment"
            app:destination="@id/createTicketOpenChatFragment" />
    </fragment>
    <fragment
        android:id="@+id/createTicketOpenChatFragment"
        android:name="com.mate.carpool.ui.fragment.us5.CreateTicketOpenChatFragment"
        android:label="fragment_create_carpool_ticket_open_chat"
        tools:layout="@layout/fragment_create_carpool_ticket_open_chat" >
        <action
            android:id="@+id/action_createTicketOpenChatFragment_to_createTicketPreviewFragment"
            app:destination="@id/createTicketPreviewFragment" />
    </fragment>
    <fragment
        android:id="@+id/createTicketPreviewFragment"
        android:name="com.mate.carpool.ui.fragment.us5.CreateTicketPreviewFragment"
        android:label="fragment_create_carpool_ticket_preview"
        tools:layout="@layout/fragment_create_carpool_ticket_preview" >
        <action
            android:id="@+id/action_createTicketPreviewFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
    </fragment>
    <fragment
        android:id="@+id/homeFragment"
        android:name="com.mate.carpool.ui.compose.HomeFragment"
        android:label="HomeFragment" >
        <action
            android:id="@+id/action_homeFragment_to_createTicketBoardingAreaFragment"
            app:destination="@id/createTicketBoardingAreaFragment" />
    </fragment>
</navigation>